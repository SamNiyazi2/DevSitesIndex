@{



    if (Program.RunWhatJavaScriptVersion == Program.whatJavaScriptVersion.Original)
    {

        <script type="module" src="~/js/Timesheet/TimesheetApp.js" asp-append-version="true"></script>
        <script type="module" src="~/js/Timesheet/TimesheetController.js" asp-append-version="true"></script>
        <script type="module" src="~/js/Timesheet/TimesheetClockOutController.js" asp-append-version="true"></script>
        <script type="module" src="~/js/Timesheet/timesheetcontinuecontroller.js" asp-append-version="true"></script>

    }


    if (Program.RunWhatJavaScriptVersion == Program.whatJavaScriptVersion.Babel_Only)
    {

        <script type="module" src="~/babel_lib/Timesheet/TimesheetApp.js" asp-append-version="true"></script>
        <script type="module" src="~/babel_lib/Timesheet/TimesheetController.js" asp-append-version="true"></script>
        <script type="module" src="~/babel_lib/Timesheet/TimesheetClockOutController.js" asp-append-version="true"></script>
        <script type="module" src="~/babel_lib/Timesheet/timesheetcontinuecontroller.js" asp-append-version="true"></script>
    }




    if (Program.RunWhatJavaScriptVersion == Program.whatJavaScriptVersion.Webpack)
    {

        <script src="~/webpack_build/bundle_timesheetapp.js" asp-append-version="true"></script>
    }



}
 


