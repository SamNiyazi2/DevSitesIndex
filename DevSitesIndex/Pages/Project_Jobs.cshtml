
@model DevSitesIndex.Pages.Projects.DetailsModel
@*
    // 04/19/2019 02:11 pm - SSN - [20190419-1411] - Adding project jobs to project details

    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@


<h3>Jobs List</h3>

<table>

    @foreach (var item in Model.project_Jobs)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateAdded)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateUpdated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobTitle)
            </td>

            <td>
                <a asp-page="./Edit" asp-route-id="@item.JobID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.JobID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.JobID">Delete</a>
            </td>
        </tr>
    }

</table>
